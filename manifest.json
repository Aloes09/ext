{
  "manifest_version": 2,

  "name": "Izak stream helper",
          "description": "Wtyczka przygotowana dla fanow Piotra 'izaka' Skowyrskiego. Pokazuje kiedy jest transmisja Live ",
  "version": "1.0",

  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
   "background": {
    "scripts": ["jquery-3.2.1.min.js","background.js"]
  },
  

  
  "permissions": [
    "activeTab",
    "storage"
  ]
}
